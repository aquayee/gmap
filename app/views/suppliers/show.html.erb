<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @supplier.name %>
</p>

<p>
  <b>Body:</b>
  <%= @supplier.body %>
</p>

<p>
  <% @supplier.addresses.each do |address| %>
    <p><%= address.addr %> lat:<%= address.lat %> lng:<%= address.lng %> <%= address.country %> <%= address.city %>
  <% end %>
</p>
<p>
  <% @supplier.contacts.each do |contact| %>
    <p><%= contact.name %> <%= contact.kind %> <%= contact.contact_no %>
  <% end %>
</p>



<%= link_to 'Edit', edit_supplier_path(@supplier) %> |
<%= link_to 'Back', suppliers_path %>
